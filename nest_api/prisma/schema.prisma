// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique
  full_name String
  avatar String
  first_time Boolean
  username String
  is_two_fa_enable Boolean
  two_fa_code String?
  email String?
}